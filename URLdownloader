#https://www.hanselman.com/blog/HowToDownloadEmbeddedVideosWithF12ToolsInYourBrowser.aspx

#!/bin/bash

CSV=urldownloader.csv

[! -f $CSV] && {echo "$CSV File Not Found"; exit 99}

while read url fname
do
        dwnt="curl $url --output $fname"
        $dwnt
done


#awk -F "\"*,\"*" '{print $2}' textfile.csv
